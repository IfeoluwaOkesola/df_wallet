{
  "name": "df_wallet",
  "displayName": "developer_foundry_project",
  "version": "1.0.0",
  "description": "Developer's Foundry Wallet System",
  "main": "src/app.ts",
  "repository": "https://github.com/IfeoluwaOkesola/df_wallet.git",
  "author": "IfeoluwaOkesola <99264083+IfeoluwaOkesola@users.noreply.github.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "lint": "eslint -c ./.eslintrc.js ./src --format stylish --fix --ext .ts",
    "start": "yarn lint && yarn dev",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:ds": "npm run typeorm -- -d ./src/config/postgres",
    "migration:create": "npm run typeorm migration:create",
    "migration:generate": "npm run typeorm:ds -- migration:generate",
    "migration:run": "npm run typeorm:ds -- migration:run",
    "migration:revert": "npm run typeorm:ds -- migration:revert"
  },
  "dependencies": {
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "chance": "^1.1.12",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "mongodb": "^6.13.0",
    "morgan": "^1.10.0",
    "ncp": "^2.0.0",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yarn": "1.22.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "eslint": "^9.20.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "typescript-eslint": "^8.24.0"
  }
}
